
        <form id="form1"  action = "" method="post">
        <h1>@ViewData["Title"]</h1>
        <h3>Форма</h3>
        <ol type="A">
			@for (int i = 0; i < 5; i++)
			{
				<li>элемент списка @(i + 1)</li>
			}
		</ol>
        <div>
            <select class="form-control" id="dropdownList" name="SelectedItemId" asp-items="@(new SelectList(ViewData["DemoList"] as List<ListDemo>, "Id", "Name"))">
            </select>
        </div>
        <div class="form-check">
            <input name ="checkbox1" class="form-check-input" type="checkbox" value="check1" form ="form1" id="flexCheckDefault">
            <label class="form-check-label" for="flexCheckDefault">
                Чекбокс 1
            </label>
        </div>
        <div class="form-check">
                <input name="checkbox2" class="form-check-input" type="checkbox" value="check2" form="form1" id="flexCheckChecked" checked>
            <label class="form-check-label" for="flexCheckChecked">
                Чекбокс 2
            </label>
        </div>
        <div class="form-check form-check-inline">
                <input name="radio" class="form-check-input" type="radio" form="form1" value = "radio1" id="flexRadioDefault1">
            <label class="form-check-label" for="flexRadioDefault1">
                Радио 1
            </label>
        </div>
            <div class="form-check form-check-inline">
                <input name="radio" class="form-check-input" type="radio" form="form1" value="radio2" id="flexRadioDefault2" checked>
            <label class="form-check-label" for="flexRadioDefault2">
                Радио 2
            </label>
        </div>
            <div class="form-check form-check-inline">
                <input name="radio" class="form-check-input" type="radio" form="form1" value="radio3" id="flexRadioDefault3" checked>
            <label class="form-check-label" for="flexRadioDefault3">
                Радио 3
            </label>
            </div>

        
                <div class="form-group">
                    <label for="exampleInputEmail1">Логин</label>
                <input name="login" form="form1"  class="form-control" id="logininput" aria-describedby="login help" placeholder="Введите имя">
                    <small id="login help" class="form-text text-muted">Лабораторная работа находится под защитой авторских прав</small>
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Пароль</label>
                <input name="password" form="form1" type="password" class="form-control" id="passwordinput" placeholder="Введите пароль">
                </div>

            <button type="submit" class="btn btn-primary mt-50">Отправить</button>
        </form>
